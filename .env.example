# Server
PORT=8080

# AWS / S3 (use IAM role in prod; local dev can rely on AWS_PROFILE)
S3_REGION=us-east-1
S3_BUCKET=your-upload-bucket

# Upload policy
UPLOAD_MAX_MB=25
UPLOAD_KEY_PREFIX=uploads
PRESIGN_EXPIRES_SEC=600
# Optional comma-separated list. If set, only these MIME types are allowed.
ALLOWED_MIME=application/pdf,image/png,image/jpeg,video/mp4
